apiVersion: apps/v1
kind: Deployment
metadata:
  name: mahendeploy
  namespace: myapp
  labels:
    app: nginx1
spec:
  replicas: 4
  selector:
    matchLabels:
      app: nginx1
  template:
    metadata:
      labels:
        app: nginx1
    spec:
      containers:
      - name: nginx
        image: nginx:1.14.2
        ports:
        - containerPort: 80
         
